      root.routes.options = defaults;
      return root.routes;
    };

    if (typeof define === "function" && define.amd) {
      define([], function () {
        return createGlobalJsRoutesObject();
      });
    } else {
      createGlobalJsRoutesObject();
    }
  }).call(undefined);

  // js-routes attaches the namespace to the exports object
  module.exports = exports.routes;
});define('components/util/send-to-splunk', ['exports', 'module', './user-agent', './user-uuid'], function (exports, module, _userAgent, _userUuid) {
  'use strict';

  function _interopRequire(obj) { return obj && obj.__esModule ? obj['default'] : obj; }

  var _userAgent2 = _interopRequire(_userAgent);

  var _userUUID = _interopRequire(_userUuid);

  var uuid = _userUUID(),
      platform = _userAgent2.platform;

  function sendToSplunk(messageType) {
    var fields = arguments[1] === undefined ? {} : arguments[1];

    var normalizedKey = undefined,
        normalizedValue = undefined;

    fields.messagType = messageType;
    fields.application = 'web';
    fields.environment = window.ENV;
    fields.uuid = uuid;
    fields.platform = platform;
    fields.url = window.location.href;

    var splunkHeader = Object.keys(fields).map(function (key) {
      normalizedKey = _.snakeCase(key);
      normalizedValue = _.snakeCase(fields[key]);

      return '' + normalizedKey + '=' + normalizedValue;
    }).join(' ');

    $.ajax({
      /* eslint-disable */
      url: 'http://web-analytics-collector.thescore.com',
      /* eslint-enable */
      method: 'GET',
      dataType: 'json',
      cache: true,
      async: true,
      headers: { 'x-splunk-data': splunkHeader }
    });
  }

  module.exports = sendToSplunk;
});define('components/util/supported-leagues', ['exports', 'module'], function (exports, module) {
  'use strict';

  var supportedLeagues = ['nba', 'ncaab', 'wcbk', 'nfl', 'cfl', 'ncaaf', 'nhl', 'mlb', 'worldcup', 'chlg', 'epl', 'mls', 'seri', 'liga', 'bund', 'fran', 'fmf', 'uefa', 'eurcq', 'mma', 'nascar', 'formula1', 'pga', 'epga', 'lpga', 'atp', 'wta', 'wolym', 'wolymhm', 'wolymhw', 'wwcup', 'copam', 'eng_fed'];

  module.exports = supportedLeagues;
});define("components/util/text-validation", ["exports", "module"], function (exports, module) {
  "use strict";

  var textValidation = {

    isEmailValid: function isEmailValid(email) {
      return /[^@\s]+@[^@\s]+\.\w+/g.test(email);
    },

    isPhoneValid: function isPhoneValid(phoneNumber) {
      return /^\d+$/.test(phoneNumber);
    }
  };

  module.exports = textValidation;
});define('components/util/user-agent', ['exports', 'module'], function (exports, module) {
  'use strict';

  var ua = navigator.userAgent,
      userAgent = {
    platform: null,
    isOS: false,
    isAndroid: false,
    isIpad: false };

  if (ua.match(/(ipad)/i)) {
    userAgent.platform = 'ios';
    userAgent.isOS = true;
    userAgent.isIpad = true;
    userAgent.isTablet = true;
    userAgent.isMobile = false;
  } else if (ua.match(/(iphone)/i)) {
    userAgent.platform = 'ios';
    userAgent.isOS = true;
  } else if (ua.match(/android/i)) {
    userAgent.platform = 'android';
    userAgent.isAndroid = true;

    // Not 100% accurate as Android does not have a unique way of detecting tablet
    // by user agent. We are using screen-sm width to decide if it is a tablet.
    if ($(window).width() >= 768) {
      userAgent.isTablet = true;
      userAgent.isMobile = false;
    }
  } else {
    userAgent.platform = 'other';
    userAgent.isMobile = false;
  }

  module.exports = userAgent;
});define('components/util/user-location', ['exports', 'module', './report-error'], function (exports, module, _reportError) {
  'use strict';

  function _interopRequire(obj) { return obj && obj.__esModule ? obj['default'] : obj; }

  var _reportError2 = _interopRequire(_reportError);

  var userLocation = {
    promise: null,
    resolved: false
  };

  function resolveLocation(resolveFunc) {
    return function (data, status, xhr) {

      userLocation.countryCode = xhr.getResponseHeader('X-Country-Code');
      userLocation.city = xhr.getResponseHeader('X-City');
      userLocation.region = xhr.getResponseHeader('X-Region');
      userLocation.metroCode = xhr.getResponseHeader('X-Metro-Code');

      userLocation.resolved = true;

      resolveFunc(userLocation);
    };
  }

  function rejectLocation(rejectFunc) {
    return function (xhr, status, error) {
      _reportError2('LocationError', 'Failed to fetch user location');
      rejectFunc(xhr, status, error);
    };
  }

  userLocation.promise = new RSVP.Promise(function (resolve, reject) {
    var locationUrl = 'http://www.thescore.com/location.json',
        settings = {
      async: true,
      type: 'GET',
      crossDomain: true,
      dataType: 'json',
      success: resolveLocation(resolve),
      error: rejectLocation(reject)
    };

    $.ajax(locationUrl, settings);
  });

  module.exports = userLocation;
});define('components/util/user-uuid', ['exports', 'module'], function (exports, module) {
  'use strict';

  function s4() {
    return Math.floor((1 + Math.random()) * 65536).toString(16).substring(1);
  }

  function newUUID() {
    var uuid = s4() + s4() + '-' + s4() + '-' + s4() + '-' + s4() + '-' + s4() + s4() + s4();

    return uuid;
  }

  function saveUUID(uuid) {
    $.cookie('tsi-uuid', uuid, { expires: 365, path: '/' });
  }

  function userUUID() {
    var id = $.cookie('tsi-uuid') || newUUID();
    saveUUID(id);
    return id;
  }

  module.exports = userUUID;
});define('components/util/youtube-analytics', ['exports', 'module'], function (exports, module) {
  'use strict';

  var YoutubeAnalytics = function YoutubeAnalytics(_config, eventsFired) {

    function checkCompletion(player, marks, videoId) {
      var currentTime = player.getCurrentTime(),
          key = undefined;

      player[videoId] = player[videoId] || {};

      for (key in marks) {
        if (marks[key] <= currentTime && !player[videoId][key]) {
          player[videoId][key] = true;
          fireAnalyticsEvent(videoId, key);
        }
      }
    }

    // Returns key/value pairs of percentages: number of seconds to achieve
    function getMarks(duration) {
      var marks = {};

      // For full support, we're handling Watch to End with percentage viewed
      if (_config.events['Watch to End']) {
        marks['Watch to End'] = duration * 99 / 100;
      }

      if (_config.percentageTracking) {
        var points = [],
            i = undefined;

        if (_config.percentageTracking.each) {
          points = points.concat(_config.percentageTracking.each);
        }

        if (_config.percentageTracking.every) {
          var every = parseInt(_config.percentageTracking.every, 10),
              num = 100 / every;

          for (i = 1; i < num; i++) {
            points.push(i * every);
          }
        }

        for (i = 0; i < points.length; i++) {
          var point = points[i],
              mark = point + '%',
              time = duration * point / 100;

          marks[mark] = Math.floor(time);
        }
      }

      return marks;
    }

    // Event handler for events emitted from the YouTube API
    function onStateChangeHandler(evt, youTubeIframe) {
      var stateIndex = evt.data,
          player = evt.target,
          targetVideoUrl = player.getVideoUrl(),
          targetVideoId = targetVideoUrl.match(/[?&]v=([^&#]*)/)[1],
          playerState = player.getPlayerState(),
          duration = player.getDuration(),
          marks = getMarks(duration),
          playerStatesIndex = {
        '1': 'Play',
        '2': 'Pause'
      },
          state = playerStatesIndex[stateIndex];

      youTubeIframe.playTracker = youTubeIframe.playTracker || {};
      if (playerState === 1 && !youTubeIframe.timer) {
        clearInterval(youTubeIframe.timer);
        youTubeIframe.timer = setInterval(function () {
          // Check every second to see if we've hit any of our percentage viewed marks
          checkCompletion(player, marks, youTubeIframe.videoId);
        }, 1000);
      } else {
        clearInterval(youTubeIframe.timer);
        youTubeIframe.timer = false;
      }

      // Playlist edge-case handler
      if (stateIndex === 1) {
        youTubeIframe.playTracker[targetVideoId] = true;
        youTubeIframe.videoId = targetVideoId;
        youTubeIframe.pauseFlag = false;
      }

      if (!youTubeIframe.playTracker[youTubeIframe.videoId]) {
        // This video hasn't started yet, so this is spam
        return false;
      }

      if (stateIndex === 2) {
        if (!youTubeIframe.pauseFlag) {
          youTubeIframe.pauseFlag = true;
        } else {
          // We don't want to fire consecutive pause events
          return false;
        }
      }

      // If we're meant to track this event, fire it
      if (eventsFired[state]) {
        fireAnalyticsEvent(youTubeIframe.videoId, state);
      }
    }

    // Returns array of elements with given tag name
    function getTagsAsArr_(tagName) {
      return [].slice.call(document.getElementsByTagName(tagName));
    }

    // Take our videos and turn them into trackable videos with events
    function digestPotentialVideos(videos) {
      var i = undefined;

      for (i = 0; i < videos.length; i++) {
        var isYouTubeVideo = checkIfYouTubeVideo(videos[i]);
        if (isYouTubeVideo) {
          var normalizedYouTubeIframe = normalizeYouTubeIframe(videos[i]);
          addYouTubeEvents(normalizedYouTubeIframe);
        }
      }
    }

    // Determine if the element is a YouTube video or not
    function checkIfYouTubeVideo(potentialYouTubeVideo) {
      var potentialYouTubeVideoSrc = potentialYouTubeVideo.src || '';

      if (potentialYouTubeVideoSrc.indexOf('youtube.com/embed/') > -1 || potentialYouTubeVideoSrc.indexOf('youtube.com/v/') > -1) {
        return true;
      }
      return false;
    }

    function jsApiEnabled(url) {
      return url.indexOf('enablejsapi') > -1;
    }

    function originEnabled(url) {
      return url.indexOf('origin') > -1;
    }

    // Turn embed objects into iframe objects and ensure they have the right parameters
    function normalizeYouTubeIframe(youTubeVideo) {
      var loc = window.location,
          a = document.createElement('a'),
          tmpPathname = undefined;

      a.href = youTubeVideo.src;
      a.hostname = 'www.youtube.com';
      a.protocol = loc.protocol;
      tmpPathname = a.pathname.charAt(0) === '/' ? a.pathname : '/' + a.pathname; // IE10 shim

      // For security reasons, YouTube wants an origin parameter set that matches our hostname
      if (!jsApiEnabled(a.search)) {
        a.search = (a.search.length > 0 ? a.search + '&' : '') + 'enablejsapi=1';
      }

      if (!originEnabled(a.search) && loc.hostname.indexOf('localhost') === -1) {
        var port = loc.port ? ':' + loc.port : '',
            origin = loc.protocol + '%2F%2F' + loc.hostname + port;

        a.search = a.search + '&origin=' + origin;
      }

      if (youTubeVideo.type === 'application/x-shockwave-flash') {
        var newIframe = document.createElement('iframe');
        newIframe.height = youTubeVideo.height;
        newIframe.width = youTubeVideo.width;
        tmpPathname = tmpPathname.replace('/v/', '/embed/');

        youTubeVideo.parentNode.parentNode.replaceChild(newIframe, youTubeVideo.parentNode);
        youTubeVideo = newIframe;
      }
      a.pathname = tmpPathname;

      if (youTubeVideo.src !== a.href + a.hash) {
        youTubeVideo.src = a.href + a.hash;
      }

      return youTubeVideo;
    }

    // Add event handlers for events emitted by the YouTube API
    function addYouTubeEvents(youTubeIframe) {
      var player = window.YT.get(youTubeIframe.id);

      if (!player) {
        player = new window.YT.Player(youTubeIframe, {});
      }

      if (typeof youTubeIframe.pauseFlag === 'undefined') {
        youTubeIframe.pauseFlag = false;
        player.addEventListener('onStateChange', function (evt) {
          onStateChangeHandler(evt, youTubeIframe);
        });
      }
    }

    // Fire an event to Google Analytics or Google Tag Manager
    function fireAnalyticsEvent(videoId, state) {
      var videoUrl = 'https://www.youtube.com/watch?v=' + videoId,
          ga = window.GoogleAnalyticsObject;

      if (typeof window.dataLayerName !== 'undefined') {
        window.dataLayerName.push({
          'event': 'youTubeTrack',
          'attributes': {
            'videoUrl': videoUrl,
            'videoAction': state
          }
        });
      } else if (typeof window[ga] === 'function' && typeof window[ga].getAll === 'function' && _config.forceSyntax !== 2) {
        window[ga]('send', 'event', 'Videos', state, videoUrl);
      } else if (typeof window._gaq !== 'undefined' && _config.forceSyntax !== 1) {
        window._gaq.push(['_trackEvent', 'Videos', state, videoUrl]);
      }
    }

    function bindToNewVideos_(evt) {
      if (typeof evt === 'undefined') {
        return;
      }

      var el = evt.target || evt.srcElement,
          isYT = checkIfYouTubeVideo(el);

      // We only bind to iframes with a YouTube URL with the enablejsapi=1 and
      // origin=<<hostname>> parameters
      if (el.tagName === 'IFRAME' && isYT && jsApiEnabled(el.src) && originEnabled(el.src)) {
        addYouTubeEvents(el);
      }
    }

    var potentialVideos = getTagsAsArr_('iframe').concat(getTagsAsArr_('embed'));
    digestPotentialVideos(potentialVideos);
    // CAPTURE NOT SUPPORTED BY IE8
    bindToNewVideos_();
  };

  module.exports = YoutubeAnalytics;
