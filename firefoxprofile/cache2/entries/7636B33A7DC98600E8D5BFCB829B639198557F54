    Bugsnag.apiKey = '1e420a723f297ad13827d7c7a74c20ad';
    Bugsnag.releaseStage = 'production';
    Bugsnag.notifyReleaseStages = ['staging', 'production'];
  };
});define("front_end/initializers/dfp-init", ["exports", "module", "components/util/dfp"], function (exports, module, _componentsUtilDfp) {
  "use strict";

  function _interopRequire(obj) { return obj && obj.__esModule ? obj["default"] : obj; }

