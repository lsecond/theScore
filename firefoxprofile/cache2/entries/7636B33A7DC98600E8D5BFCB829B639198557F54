define('front_end/initializers/ad-block-tracker', ['exports', 'module'], function (exports, module) {
  'use strict';

  module.exports = function () {
    $(document).ready(function () {
      setTimeout(function () {
        var detected = 'No';
        var el = document.getElementById('adsense');
        if (!el || el.innerHTML.length === 0 || el.clientHeight === 0) {
          detected = 'Yes';
        }
        ga('send', 'event', 'AdBlock', detected, {
          nonInteraction: true
        });
      }, 2000);
    });
  };
});define('front_end/initializers/article-infinite-scroll', ['exports', 'module', 'components/util/send-to-splunk'], function (exports, module, _componentsUtilSendToSplunk) {
  'use strict';

  function _interopRequire(obj) { return obj && obj.__esModule ? obj['default'] : obj; }

  var _sendToSplunk = _interopRequire(_componentsUtilSendToSplunk);

  var $articles = undefined,
      $relatedArticles = undefined,
      $activeRelatedArticle = undefined,
      $prevRelatedArticle = undefined,
      numArticles = undefined;

  // Called after each pagination
  var jScrollCallbackFn = function jScrollCallbackFn() {
    $('.article-segment--embed').fitVids();

    ReactRailsUJS.mountComponents();

    dfp.reevaluate();

    refreshArticleData();
  };

  var removeEvents = function removeEvents() {
    $(window).off('resize.article_show', setRailHeight);
    $(window).off('scroll.article_show', throttleScrollEvent);
    $(window).off('popstate.article_show', onUrlChange);
  };

  var setRailHeight = function setRailHeight() {
    var offset = $('.related-articles').offset().top - $(window).scrollTop();
    $('.related-articles').height($(window).height() - offset);
  };

  var init = function init() {
    var $articleInfiniteScroll = $('.article-infinite-scroll'),
        $articleTilesInfiniteScroll = $('.related-articles'),
        distanceFromBottom = 100;

    if (!$articleInfiniteScroll.length) {
      return;
    }

    var firstArticle = $('.article-sidebar-tile:first');
    firstArticle.removeClass('upcoming');
    firstArticle.addClass('active');

    $articleInfiniteScroll.jscroll({
      padding: distanceFromBottom,
      autoTrigger: true,
      nextSelector: 'a.jscroll-next:last',
      loadingHtml: '<div class="loader"></div>',
      callback: jScrollCallbackFn
    });

    $articleTilesInfiniteScroll.jscroll({
      nextSelector: 'a.jscroll-tile-next:last',
      loadingHtml: '<div class="loader"></div>',
      callback: jScrollCallbackFn
    });

    $('.jscroll-tile-next').click();

    setRailHeight();

    $(window).on('resize.article_show', setRailHeight);
    $(window).on('scroll.article_show', throttleScrollEvent);

    // Need to be executed only for the first article.
    // The rest of the videos are handled by the popstate event, when visible on viewport.
    $(document).one('videoLoaded.article_show', onUrlChange);
    $(window).on('popstate.article_show', onUrlChange);
  };

  // Get article data once new articles are loaded
  var refreshArticleData = function refreshArticleData() {
    $articles = $('.article-contents-wrapper');
    $relatedArticles = $('.article-sidebar-tile');
    numArticles = $articles.length;
  };

  // Sets 'active' class to matching related article tile. Sets 'completed' class
  // to all previous tiles.
  var setActiveRelatedArticle = function setActiveRelatedArticle($relatedArticles, $activeRelatedArticle) {
    $relatedArticles.removeClass('active');
    $relatedArticles.addClass('upcoming');

    // Add a complete class to previous article. The prevAll doesn't work when the prvious element
    // was the last loaded in the batch by jscroll, since every batch is loaded in a new container
    if ($prevRelatedArticle) {
      if ($prevRelatedArticle[0] != $activeRelatedArticle[0] && $prevRelatedArticle.offset().top < $activeRelatedArticle.offset().top) {
        $prevRelatedArticle.addClass('completed');
      }
    }

    $activeRelatedArticle.addClass('active');
    $activeRelatedArticle.removeClass('upcoming');
    $activeRelatedArticle.removeClass('completed');
    $activeRelatedArticle.prevAll().addClass('completed');

    if ($activeRelatedArticle.index() > 5 && !$activeRelatedArticle.parent().attr('loadedMore')) {
      $activeRelatedArticle.parent().attr('loadedMore', true);
      $('.jscroll-tile-next').click();
      setActiveRelatedArticleSidebarPosition($activeRelatedArticle, 0);
    }
  };

  // Sets progress height to active related article tile
  var setActiveRelatedArticleProgress = function setActiveRelatedArticleProgress(windowTop, offset, $activeArticle, $activeRelatedArticle) {
    var activeArticleHeight = $activeArticle.outerHeight(),
        activeArticleTop = $activeArticle.offset().top - offset;

    var heightPercentage = (windowTop - activeArticleTop) / activeArticleHeight * 100;

    if (heightPercentage < 0) {
      heightPercentage = 0;
    } else if (heightPercentage > 100) {
      heightPercentage = 100;
    }

    $activeRelatedArticle.find('.article-sidebar-tile__progress').height('' + heightPercentage + '%');
  };

  var setActiveRelatedArticleSidebarPosition = function setActiveRelatedArticleSidebarPosition($activeRelatedArticle) {
    var duration = arguments[1] === undefined ? 300 : arguments[1];

    var parent = $activeRelatedArticle.parent();

    // Set the slidebar position to where the active article is second in the list
    // Don't move the slidebar if the article is the very first.
    if ($activeRelatedArticle.index() > 0 && parent.index() == 0 || parent.index() > 0) {
      var relatedArticlesContainer = $('.related-articles'),
          articleTileHeight = $('.article-sidebar-tile').first().outerHeight() - 1,
          scrollTopPosition = $activeRelatedArticle.index() * articleTileHeight,
          prevContainersHeight = $activeRelatedArticle.parent().height() * ($activeRelatedArticle.parent().index() - 1);

      relatedArticlesContainer.stop();
      relatedArticlesContainer.animate({
        scrollTop: scrollTopPosition + prevContainersHeight
      }, duration);
    }
  };

  var isElementInViewport = function isElementInViewport(el) {
    var rect = el.getBoundingClientRect();

    return rect.top >= 0 && rect.left >= 0 && rect.bottom <= ($(window).innerHeight() || $(window).height()) && rect.right <= ($(window).innerWidth() || $(window).width());
  };

  var activeVideoChanged = function activeVideoChanged($video) {
    if (isElementInViewport($video.get(0))) {
      $video.trigger('videoVisible');
    }
  };

  var onScroll = function onScroll() {
    var windowTop = $(this).scrollTop(),
        offset = $('.main-menu-row').outerHeight();

    if ($('.article-contents-wrapper').length !== numArticles) {
      refreshArticleData();
    }

    var $activeArticle = $($.grep($articles, function (item) {
      return $(item).is(':visible') && $(item).offset().top - offset <= windowTop;
    })).last();

    // The offset for the first article is different than the rest.
    if (!$activeArticle.length) {
      $activeArticle = $articles.eq(0);
    }

    $articles.removeClass('active');
    $activeArticle.addClass('active');
    $activeArticle.removeClass('upcoming');

    /* eslint-disable one-var */
    var currentUrl = window.location.pathname,
        activeArticlePath = $activeArticle.data('src'),
        $activeRelatedArticle = $('.article-sidebar-tile[data-src="' + activeArticlePath + '"]');
    /* eslint-enable one-var */

    if ($activeRelatedArticle.length) {
      setActiveRelatedArticle($relatedArticles, $activeRelatedArticle);
      setActiveRelatedArticleProgress(windowTop, offset, $activeArticle, $activeRelatedArticle);
      $prevRelatedArticle = $activeRelatedArticle;
    }

    // Update history
    if (currentUrl !== activeArticlePath) {
      // Set RelatedArticle to active and fix position of scroll
      if ($activeRelatedArticle.length) {
        setActiveRelatedArticleSidebarPosition($activeRelatedArticle);
      }

      history.replaceState(null, null, activeArticlePath);

      // Page View
      if ($activeArticle.attr('data-viewed') === 'false') {
        dfp.refreshSlotById('ad-sidebar-big-box');

        ga('send', 'pageview', window.location.pathname, {
          'dimension1': 'yes'
        });

        _sendToSplunk('pageview');

        $activeArticle.attr('data-viewed', 'true');
      }

      $(window).trigger('popstate', { $articleVideo: $('.article-video', $activeArticle) });
    }
  };

  var throttleScrollEvent = _.throttle(onScroll, 300);

  var onUrlChange = function onUrlChange(event, data) {
    var $articleVideo = data && data.$articleVideo;

    if ($articleVideo.length) {
      activeVideoChanged($articleVideo);
    }
  };

  module.exports = function () {
    $(document).on('page:before-unload', removeEvents).on('ready page:restore page:load', init);
  };
});define('front_end/initializers/branch-app-check', ['exports', 'module'], function (exports, module) {
  'use strict';

  module.exports = function () {
    $(document).ready(function () {
      if ($(window).width() < 769) {
        (function () {
          var app_installed = 'No';
          setTimeout(function () {
            branch.data(function (err, data) {
              if (data.has_app == true) {
                app_installed = 'Yes';
              }
            });
            ga('send', 'event', 'AppCheck', app_installed, {
              nonInteraction: true
            });
          }, 2000);
        })();
      }
    });
  };
});define('front_end/initializers/bugsnag', ['exports', 'module'], function (exports, module) {
