});define('components/util/meta-leagues', ['exports', 'module', './report-error', './api-get', './supported-leagues'], function (exports, module, _reportError, _apiGet, _supportedLeagues) {
  'use strict';

  function _interopRequire(obj) { return obj && obj.__esModule ? obj['default'] : obj; }

  var _apiGet2 = _interopRequire(_apiGet);

  var _supportedLeagues2 = _interopRequire(_supportedLeagues);

  var leagues = {
    promise: null
  };

  var apiOptions = {
    headers: {
      'X-Api-Version': '1.7.10',
      'X-Country-Code': 'CA',
      'X-User-Agent': 'theScore (Web)'
    }
  };

  function supported(league) {
    return _.includes(_supportedLeagues2, league.slug);
  }

  function resolveLeagues(resolveFunc) {
    return function (data) {
      leagues.show = data.leagues.show;
      leagues.more = data.leagues.more;
      leagues.excluded = data.leagues.excluded;
      leagues.all = leagues.show.concat(leagues.more);
      leagues.supported = leagues.all.filter(supported);

      resolveFunc(leagues);
    };
  }

  function rejectLeagues(rejectFunc) {
    return function (xhr, status, error) {
      _reportError.reportError('LeaguesError', 'Failed to fetch meta leagues');
      rejectFunc(xhr, status, error);
    };
  }

  leagues.promise = new RSVP.Promise(function (resolve, reject) {
    var leaguesUrl = '/meta/leagues';

    return _apiGet2(leaguesUrl, apiOptions).then(resolveLeagues(resolve), rejectLeagues(reject));
  });

  module.exports = leagues;
});define('components/util/spotlights', ['exports', 'module', '../util/api-get'], function (exports, module, _utilApiGet) {
