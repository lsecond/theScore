        if (query) {
          _apiGet('/search', {
            data: {
              q: query,
              size: 20
            }
          }).then(function (results) {
            groupedResults = _.groupBy(results, 'document_type');

            if (_.isEmpty(groupedResults)) {
              _this2.setState({ results: null });
            } else {
              _this2.setState({ results: groupedResults });
            }
          });
        } else {
          this.setState({
            query: '',
            results: null
          });
        }
      }
    }]);

