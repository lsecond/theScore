        frontEndArticlePath: Utils.route(["article_id"], ["format"], [2, [2, [2, [2, [7, "/", false], [6, "news", false]], [7, "/", false]], [3, "article_id", false]], [1, [2, [8, ".", false], [3, "format", false]], false]], arguments),
