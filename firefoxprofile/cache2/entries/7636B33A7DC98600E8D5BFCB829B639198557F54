                { className: 'modal-content' },
                React.createElement(
                  'div',
                  { className: 'modal-header' },
                  React.createElement(
                    'button',
                    { type: 'button', className: 'close', 'data-dismiss': 'modal', 'aria-label': 'Close' },
                    React.createElement(
