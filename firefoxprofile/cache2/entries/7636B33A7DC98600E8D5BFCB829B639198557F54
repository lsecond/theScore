      }
    }, {
      key: 'loadEntries',
      value: function loadEntries() {
        var _this2 = this;

        var urlBase = this.props.entriesUrlBase,
            urlPath = this.props.entriesPath,
            url = urlBase + urlPath,
            currentEntriesId = _.map(this.state.entries, 'id');

        var newEntries = undefined,
            newEntriesArray = undefined;

        $.ajax({
          dataType: 'json',
          url: url,
          success: function success(data) {
            newEntries = _.filter(data.live_blog_entries, function (entry) {
              return !_.includes(currentEntriesId, entry.id);
            });

            newEntriesArray = newEntries.concat(_this2.state.entries.slice());

            _this2.setState({
              entries: _.sortByOrder(newEntriesArray, 'id', false)
            });
          }
        });
      }
    }, {
      key: 'loadMore',
      value: function loadMore() {
        var _this3 = this;

        var urlBase = this.props.entriesUrlBase,
            urlPath = this.props.entriesPath,
            url = urlBase + urlPath,
            lastLiveBlogEntryId = this.state.entries[this.state.entries.length - 1].id;

        $.ajax({
          dataType: 'json',
          url: url,
          data: {
            infinite_scroll_id: lastLiveBlogEntryId // eslint-disable-line camelcase
          },
          success: function success(data) {
            _this3.setState({
              entries: _this3.state.entries.concat(data.live_blog_entries)
            });

            if (_.isNull(data.meta.pagination.infinite_scroll_id)) {
              _this3.setState({
                loadMore: false
              });
            }
          }
        });
      }
    }, {
      key: 'parseEmbeds',
