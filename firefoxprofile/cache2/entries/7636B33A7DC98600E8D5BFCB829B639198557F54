});define('components/util/dfp', ['exports', 'module'], function (exports, module) {
  // Based on http://reed.github.io/turbolinks-compatibility/doubleclick_for_publishers.html

  'use strict';

  var DoubleclickForPublishers = undefined;

  DoubleclickForPublishers = function (options) {
    this.networkId = options.networkId;
    this.adUnitBase = options.adUnitBase;

    this.selector = '.ad-outlet';
    this.slots = {};

    this.debug = false;

    this.retryCount = 0;
    this.maxRetries = 10;

    this.init();
  };

  DoubleclickForPublishers.prototype = {

    init: function init() {
      var _this = this;

      if (!this.isApiReady()) {
        this.debug && console.log('dfp', 'init()', 'api not ready'); // eslint-disable-line no-unused-expressions, no-console

        if (this.retryCount < this.maxRetries) {
          this.debug && console.log('dfp', 'init()', 'attempt #', this.retryCount); // eslint-disable-line no-unused-expressions, no-console
          this.retryCount++;

          setTimeout(function () {
            _this.init();
          }, 1000);
        } else {
          this.debug && console.log('dfp', 'init()', 'aborted'); // eslint-disable-line no-unused-expressions, no-console
        }

        return;
      }

      this.debug && console.log('dfp', 'init()', 'ready'); // eslint-disable-line no-unused-expressions, no-console

      this.evaluate();
    },

    isApiReady: function isApiReady() {
      return typeof googletag !== 'undefined' && googletag !== null && googletag.apiReady === true;
    },

    // To be called by init(). Defines initial slots, enable services and display
    evaluate: function evaluate() {
      var _this2 = this;

      googletag.cmd.push(function () {
        _this2.defineSlots();

        googletag.enableServices();

        _this2.displaySlots();
      });
    },

    // Returns a googletag SizeMapping object based on Bootstrap's grid
    getSizeMapping: function getSizeMapping(adSizes, viewport) {
      var isVisibleXs = viewport.indexOf('visible-xs') > -1,
          isVisibleSm = viewport.indexOf('visible-sm') > -1,
          isVisibleMd = viewport.indexOf('visible-md') > -1,
          isVisibleLg = viewport.indexOf('visible-lg') > -1;

      return googletag.sizeMapping().addSize([0, 0], []).addSize([320, 0], isVisibleXs ? adSizes : []).addSize([768, 0], isVisibleSm ? adSizes : []).addSize([992, 0], isVisibleMd ? adSizes : []).addSize([1200, 0], isVisibleLg ? adSizes : []).build();
    },

    // Destroys all slots using googletag.destroySlots()
    // Doesn't remove the slots from the this.slots hash or removes the wrap
    destroySlots: function destroySlots() {
      googletag.cmd.push(function () {
        googletag.destroySlots();
      });
    },

    // Loops through slots and calls displaySlot()
    displaySlots: function displaySlots() {
      var slotIds = Object.keys(this.slots),
          self = this;

      $(slotIds).each(function (index, id) {
        self.displaySlot(id);
      });
    },

    // Calls googletag.display() if element exists and has no children inside
    displaySlot: function displaySlot(elementId) {
      var $el = $('#' + elementId),
          self = this;

      // If the element exists and have no contents
      if ($el.length > 0 && $el.children().length === 0) {
        self.debug && console.log('dfp', 'displaySlot()', 'calling display on ' + elementId); // eslint-disable-line no-unused-expressions, no-console

        googletag.display(elementId);
      }
    },

    // Goes through the page for .ad-outlets and calls defineSlotById()
    defineSlots: function defineSlots() {
      var $slotDivs = $(this.selector),
          self = this;

      $slotDivs.each(function (index, el) {
        self.defineSlotById(el.id);
      });
    },

    // Calls defineSlot() on the id if it doesn't already exist in slots
    // Also sets targetting on the slots
    defineSlotById: function defineSlotById(elementId) {
      if (this.slots[elementId]) {
        this.debug && console.log('dfp', 'defineSlotById()', elementId + ' is already defined'); // eslint-disable-line no-unused-expressions, no-console

        return;
      }

      var $el = $('#' + elementId);

      if ($el.length > 0) {
        this.slots[elementId] = this.defineSlot($el).setTargeting('league', $el.attr('data-league')).setTargeting('section', $el.attr('data-section')).setTargeting('articleId', $el.attr('data-article-id'));

        this.debug && console.log('dfp', 'defineSlotById()', elementId + ' slot added'); // eslint-disable-line no-unused-expressions, no-console
      } else {
        this.debug && console.log('dfp', 'defineSlotById()', elementId + ' not found'); // eslint-disable-line no-unused-expressions, no-console
      }
    },

    // Defines the actual slot
    defineSlot: function defineSlot(adElement) {
      var id = adElement.attr('id'),
          isOutOfPage = adElement.attr('data-out-of-page'),
          adUnit = adElement.attr('data-ad-unit'),
          path = '/' + this.networkId + '/' + this.adUnitBase + '-' + adUnit,
          size = JSON.parse(adElement.attr('data-ad-size')),
          viewport = adElement.attr('data-viewport'),
          sizeMapping = this.getSizeMapping(size, viewport);

      var adSlot = undefined;

      if (isOutOfPage === 'true') {
        adSlot = googletag.defineOutOfPageSlot(path, id);
      } else {
        adSlot = googletag.defineSlot(path, size, id);
      }

      adSlot.defineSizeMapping(sizeMapping).setCollapseEmptyDiv(true).addService(googletag.pubads());

      return adSlot;
    },

    // Refresh an ad slot
    refreshSlotById: function refreshSlotById(elementId) {
      var adSlot = this.slots[elementId];

      if (!adSlot) {
        this.debug && console.log('dfp', 'refreshSlot()', elementId + ' not defined'); // eslint-disable-line no-unused-expressions, no-console

        return;
      }

      this.debug && console.log('dfp', 'refreshSlot()', 'refreshing ' + elementId); // eslint-disable-line no-unused-expressions, no-console

      googletag.pubads().clear([adSlot]);
      googletag.pubads().refresh([adSlot]);
    },

    // Removes wrap
    clearWrap: function clearWrap() {
      var $wrapContainer = $('.wrapped-content');

      if ($wrapContainer.length > 0) {
        $wrapContainer[0].style = '';
      }

      $('.wrap-link').each(function () {
        this.href = '';
        this.className.replace('show', 'hide');
      });
    },

    reevaluate: function reevaluate() {
      var self = this;

      this.debug && console.log('dfp', 'reevaluate()', 're-defining slots and calling display'); // eslint-disable-line no-unused-expressions, no-console

      googletag.cmd.push(function () {
        self.defineSlots();
        self.displaySlots();
      });
    },

    // Destroys all slots, removes wrap and resets slots hash
    clear: function clear() {
      this.debug && console.log('dfp', 'clear()', 'clearing all ads'); // eslint-disable-line no-unused-expressions, no-console

      this.destroySlots();
      this.clearWrap();
      this.slots = {};
    }
  };

  module.exports = DoubleclickForPublishers;
});define('components/util/event-tracking', ['exports', 'module', './send-to-splunk', './call-kontagent'], function (exports, module, _sendToSplunk, _callKontagent) {
  'use strict';

  function _interopRequire(obj) { return obj && obj.__esModule ? obj['default'] : obj; }

  var _sendToSplunk2 = _interopRequire(_sendToSplunk);

  var _callKontagent2 = _interopRequire(_callKontagent);

  var EventTracking = {
    send: function send(category, action) {
      var options = arguments[2] === undefined ? {} : arguments[2];

      var url = window.location.href,
          splunkData = {
        category: category,
        action: action
      };

      ga('send', 'event', category, action, options);

      _sendToSplunk2('event', splunkData);

      _callKontagent2(category, action, url);
    }
  };

  module.exports = EventTracking;
});define('components/util/report-error', ['exports', './send-to-splunk'], function (exports, _sendToSplunk) {
  'use strict';

  Object.defineProperty(exports, '__esModule', {
    value: true
  });
  exports.reportError = reportError;
  exports.reportException = reportException;

  function _interopRequire(obj) { return obj && obj.__esModule ? obj['default'] : obj; }

  var _sendToSplunk2 = _interopRequire(_sendToSplunk);

  function reportError(errorName, message, severity) {
    _sendToSplunk2('error', {
      severity: severity,
      errorName: errorName,
      message: message
    });

    Bugsnag.notify(errorName, message, {}, severity);
  }

  function reportException(exception, customErrorName) {
    _sendToSplunk2('exception', {
      exception: exception,
      customErrorName: customErrorName
    });

    Bugsnag.notifyException(exception, customErrorName);
  }
});define("components/util/routes", ["exports", "module"], function (exports, module) {
  "use strict";

  (function () {
    var NodeTypes,
        ParameterMissing,
        Utils,
        createGlobalJsRoutesObject,
        defaults,
        root,
        __hasProp = ({}).hasOwnProperty;

    root = typeof exports !== "undefined" && exports !== null ? exports : this;

    ParameterMissing = function (message) {
      this.message = message;
    };

    ParameterMissing.prototype = new Error();

    defaults = {
      prefix: "",
      default_url_options: {}
    };

    NodeTypes = { "GROUP": 1, "CAT": 2, "SYMBOL": 3, "OR": 4, "STAR": 5, "LITERAL": 6, "SLASH": 7, "DOT": 8 };

    Utils = {
      serialize: function serialize(object, prefix) {
        var element, i, key, prop, result, s, _i, _len;

        if (prefix == null) {
          prefix = null;
        }
        if (!object) {
          return "";
        }
        if (!prefix && !(this.get_object_type(object) === "object")) {
          throw new Error("Url parameters should be a javascript hash");
        }
        if (root.jQuery) {
          result = root.jQuery.param(object);
          return !result ? "" : result;
        }
        s = [];
        switch (this.get_object_type(object)) {
          case "array":
            for (i = _i = 0, _len = object.length; _i < _len; i = ++_i) {
              element = object[i];
              s.push(this.serialize(element, prefix + "[]"));
            }
            break;
          case "object":
            for (key in object) {
              if (!__hasProp.call(object, key)) continue;
              prop = object[key];
              if (!(prop != null)) {
                continue;
              }
              if (prefix != null) {
                key = "" + prefix + "[" + key + "]";
              }
              s.push(this.serialize(prop, key));
            }
            break;
          default:
            if (object) {
              s.push("" + encodeURIComponent(prefix.toString()) + "=" + encodeURIComponent(object.toString()));
            }
        }
        if (!s.length) {
          return "";
        }
        return s.join("&");
      },
      clean_path: function clean_path(path) {
        var last_index;

        path = path.split("://");
        last_index = path.length - 1;
        path[last_index] = path[last_index].replace(/\/+/g, "/");
        return path.join("://");
      },
      set_default_url_options: function set_default_url_options(optional_parts, options) {
        var i, part, _i, _len, _results;

        _results = [];
        for (i = _i = 0, _len = optional_parts.length; _i < _len; i = ++_i) {
          part = optional_parts[i];
          if (!options.hasOwnProperty(part) && defaults.default_url_options.hasOwnProperty(part)) {
            _results.push(options[part] = defaults.default_url_options[part]);
          }
        }
        return _results;
      },
      extract_anchor: function extract_anchor(options) {
        var anchor;

        anchor = "";
        if (options.hasOwnProperty("anchor")) {
          anchor = "#" + options.anchor;
          delete options.anchor;
        }
        return anchor;
      },
      extract_trailing_slash: function extract_trailing_slash(options) {
        var trailing_slash;

        trailing_slash = false;
        if (defaults.default_url_options.hasOwnProperty("trailing_slash")) {
          trailing_slash = defaults.default_url_options.trailing_slash;
        }
        if (options.hasOwnProperty("trailing_slash")) {
          trailing_slash = options.trailing_slash;
          delete options.trailing_slash;
        }
        return trailing_slash;
      },
      extract_options: function extract_options(number_of_params, args) {
        var last_el;

        last_el = args[args.length - 1];
        if (args.length > number_of_params || last_el != null && "object" === this.get_object_type(last_el) && !this.look_like_serialized_model(last_el)) {
          return args.pop();
        } else {
          return {};
        }
      },
      look_like_serialized_model: function look_like_serialized_model(object) {
        return "id" in object || "to_param" in object;
      },
      path_identifier: function path_identifier(object) {
        var property;

        if (object === 0) {
          return "0";
        }
        if (!object) {
          return "";
        }
        property = object;
        if (this.get_object_type(object) === "object") {
          if ("to_param" in object) {
            property = object.to_param;
          } else if ("id" in object) {
            property = object.id;
          } else {
            property = object;
          }
          if (this.get_object_type(property) === "function") {
            property = property.call(object);
          }
        }
        return property.toString();
      },
      clone: function clone(obj) {
        var attr, copy, key;

        if (obj == null || "object" !== this.get_object_type(obj)) {
          return obj;
        }
        copy = obj.constructor();
        for (key in obj) {
          if (!__hasProp.call(obj, key)) continue;
          attr = obj[key];
          copy[key] = attr;
        }
        return copy;
      },
      prepare_parameters: function prepare_parameters(required_parameters, actual_parameters, options) {
        var i, result, val, _i, _len;

        result = this.clone(options) || {};
        for (i = _i = 0, _len = required_parameters.length; _i < _len; i = ++_i) {
          val = required_parameters[i];
          if (i < actual_parameters.length) {
            result[val] = actual_parameters[i];
          }
        }
        return result;
      },
      build_path: function build_path(required_parameters, optional_parts, route, args) {
        var anchor, opts, parameters, result, trailing_slash, url, url_params;

        args = Array.prototype.slice.call(args);
        opts = this.extract_options(required_parameters.length, args);
        if (args.length > required_parameters.length) {
          throw new Error("Too many parameters provided for path");
        }
        parameters = this.prepare_parameters(required_parameters, args, opts);
        this.set_default_url_options(optional_parts, parameters);
        anchor = this.extract_anchor(parameters);
        trailing_slash = this.extract_trailing_slash(parameters);
        result = "" + this.get_prefix() + this.visit(route, parameters);
        url = Utils.clean_path("" + result);
        if (trailing_slash === true) {
          url = url.replace(/(.*?)[\/]?$/, "$1/");
        }
        if ((url_params = this.serialize(parameters)).length) {
          url += "?" + url_params;
        }
        url += anchor;
        return url;
      },
      visit: function visit(route, parameters, optional) {
        var left, left_part, right, right_part, type, value;

        if (optional == null) {
          optional = false;
        }
        type = route[0], left = route[1], right = route[2];
        switch (type) {
          case NodeTypes.GROUP:
            return this.visit(left, parameters, true);
          case NodeTypes.STAR:
            return this.visit_globbing(left, parameters, true);
          case NodeTypes.LITERAL:
          case NodeTypes.SLASH:
          case NodeTypes.DOT:
            return left;
          case NodeTypes.CAT:
            left_part = this.visit(left, parameters, optional);
            right_part = this.visit(right, parameters, optional);
            if (optional && !(left_part && right_part)) {
              return "";
            }
            return "" + left_part + right_part;
          case NodeTypes.SYMBOL:
            value = parameters[left];
            if (value != null) {
              delete parameters[left];
              return this.path_identifier(value);
            }
            if (optional) {
              return "";
            } else {
              throw new ParameterMissing("Route parameter missing: " + left);
            }
            break;
          default:
            throw new Error("Unknown Rails node type");
        }
      },
      build_path_spec: function build_path_spec(route, wildcard) {
        var left, right, type;

        if (wildcard == null) {
          wildcard = false;
        }
        type = route[0], left = route[1], right = route[2];
        switch (type) {
          case NodeTypes.GROUP:
            return "(" + this.build_path_spec(left) + ")";
          case NodeTypes.CAT:
            return "" + this.build_path_spec(left) + this.build_path_spec(right);
          case NodeTypes.STAR:
            return this.build_path_spec(left, true);
          case NodeTypes.SYMBOL:
            if (wildcard === true) {
              return "" + (left[0] === "*" ? "" : "*") + left;
            } else {
              return ":" + left;
            }
            break;
          case NodeTypes.SLASH:
          case NodeTypes.DOT:
          case NodeTypes.LITERAL:
            return left;
          default:
            throw new Error("Unknown Rails node type");
        }
      },
      visit_globbing: function visit_globbing(route, parameters, optional) {
        var left, right, type, value;

        type = route[0], left = route[1], right = route[2];
        if (left.replace(/^\*/i, "") !== left) {
          route[1] = left = left.replace(/^\*/i, "");
        }
        value = parameters[left];
        if (value == null) {
          return this.visit(route, parameters, optional);
        }
        parameters[left] = (function () {
          switch (this.get_object_type(value)) {
            case "array":
              return value.join("/");
            default:
              return value;
          }
        }).call(this);
        return this.visit(route, parameters, optional);
      },
      get_prefix: function get_prefix() {
        var prefix;

        prefix = defaults.prefix;
        if (prefix !== "") {
          prefix = prefix.match("/$") ? prefix : "" + prefix + "/";
        }
        return prefix;
      },
      route: function route(required_parts, optional_parts, route_spec) {
        var path_fn;

        path_fn = function () {
          return Utils.build_path(required_parts, optional_parts, route_spec, arguments);
        };
        path_fn.required_params = required_parts;
        path_fn.toString = function () {
          return Utils.build_path_spec(route_spec);
        };
        return path_fn;
      },
      _classToTypeCache: null,
      _classToType: function _classToType() {
        var name, _i, _len, _ref;

        if (this._classToTypeCache != null) {
          return this._classToTypeCache;
        }
        this._classToTypeCache = {};
        _ref = "Boolean Number String Function Array Date RegExp Object Error".split(" ");
        for (_i = 0, _len = _ref.length; _i < _len; _i++) {
          name = _ref[_i];
          this._classToTypeCache["[object " + name + "]"] = name.toLowerCase();
        }
        return this._classToTypeCache;
      },
      get_object_type: function get_object_type(obj) {
        if (root.jQuery && root.jQuery.type != null) {
          return root.jQuery.type(obj);
        }
        if (obj == null) {
          return "" + obj;
        }
        if (typeof obj === "object" || typeof obj === "function") {
          return this._classToType()[Object.prototype.toString.call(obj)] || "object";
        } else {
          return typeof obj;
        }
      }
    };

    createGlobalJsRoutesObject = function () {
      var namespace;

      namespace = function (mainRoot, namespaceString) {
        var current, parts;

        parts = namespaceString ? namespaceString.split(".") : [];
        if (!parts.length) {
          return;
        }
        current = parts.shift();
        mainRoot[current] = mainRoot[current] || {};
        return namespace(mainRoot[current], parts.join("."));
      };
      namespace(root, "routes");
      root.routes = {
        // front_end => /:league(.:format)
        // function(league, options)
        frontEndPath: Utils.route(["league"], ["format"], [2, [2, [7, "/", false], [3, "league", false]], [1, [2, [8, ".", false], [3, "format", false]], false]], arguments),
        // front_end_ad => /ad(.:format)
        // function(options)
