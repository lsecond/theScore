        frontEndClientPagesCopyrightThescoreesportsPath: Utils.route([], ["format"], [2, [2, [2, [2, [2, [2, [2, [2, [7, "/", false], [6, "pages", false]], [7, "/", false]], [6, "mobile", false]], [7, "/", false]], [6, "copyright", false]], [7, "/", false]], [6, "thescoreesports", false]], [1, [2, [8, ".", false], [3, "format", false]], false]], arguments),
