  var _metaLeagues = _interopRequire(_componentsUtilMetaLeagues);

  var _logoTemplate = _interopRequire(_componentsUtilLogoTemplate);

  var normalizedPlatform = _userAgent.platform === 'other' ? 'android' : _userAgent.platform;

  function findSportByLeagueSlug(leaguesArray, leagueSlug) {
    var league = _.find(leaguesArray, { 'slug': leagueSlug });

    return league ? league.sport_name : 'default';
  }

  var InterstitialAd = (function (_React$Component) {
    function InterstitialAd() {
      _classCallCheck(this, InterstitialAd);

      _get(Object.getPrototypeOf(InterstitialAd.prototype), 'constructor', this).call(this);

      this.state = {
        visible: false
      };

      this.closeModal = this.closeModal.bind(this);
      this.clickGetTheApp = this.clickGetTheApp.bind(this);
    }

    _inherits(InterstitialAd, _React$Component);

    _createClass(InterstitialAd, [{
      key: 'interstitialCssClasses',
      value: function interstitialCssClasses() {
        var cssClasses = ['interstitial', 'interstitial--' + normalizedPlatform];

        if (!this.state.visible) {
          cssClasses.push('hidden');
        }

        return cssClasses.join(' ');
      }
    }, {
      key: 'getImageVersions',
      value: function getImageVersions() {
        var imageUrls = this.props.imageUrls,
            imageVersions = imageUrls[normalizedPlatform][this.props.currentLeague] || imageUrls[normalizedPlatform][this.currentSport] || imageUrls[normalizedPlatform]['default'];

        return imageVersions;
      }
    }, {
      key: 'getIpadImageVersions',
      value: function getIpadImageVersions() {
        var imageUrls = this.props.imageUrls,
            imageVersions = imageUrls[normalizedPlatform]['default'];

        if (_.contains(['nba', 'nfl', 'nhl', 'soccer'], this.props.currentLeague) || _.contains(['soccer'], this.currentSport)) {
          imageVersions = imageUrls[normalizedPlatform][this.props.currentLeague] || imageUrls[normalizedPlatform][this.currentSport];
        }

        return imageVersions;
      }
    }, {
      key: 'setImageUrl',
      value: function setImageUrl() {
        this.setState({ imageUrl: this.getImageVersions().w434 });
      }
    }, {
      key: 'setSrcset',
      value: function setSrcset() {
        var imageVersions = this.getImageVersions(),
            variants = [{ size: '174w', version: imageVersions.w174 }, { size: '347w', version: imageVersions.w347 }, { size: '434w', version: imageVersions.w434 }],
            srcset = variants.map(function (variant) {
          return '' + variant.version + ' ' + variant.size;
        }).join(', \n');

        this.setState({ srcset: srcset });
      }
    }, {
      key: 'setIpadSrcset',
      value: function setIpadSrcset() {
        var imageVersions = this.getIpadImageVersions();

        this.setState({ srcset: imageVersions.w600 + ' 600w' });
      }
    }, {
      key: 'setMessage',
      value: function setMessage() {
        var messages = this.props.messages,
            message = messages[this.props.currentLeague] || messages[this.currentSport] || messages['default'];

        this.setState({ message: message });
      }
    }, {
      key: 'setIpadMessage',
      value: function setIpadMessage() {
        var messages = this.props.messages,
            message = messages['default'];

        if (_.contains(['nba', 'nfl', 'nhl', 'soccer'], this.props.currentLeague) || _.contains(['soccer'], this.currentSport)) {
          message = messages[this.props.currentLeague] || messages[this.currentSport];
        }

        this.setState({ message: message });
      }
    }, {
      key: 'setGetTheAppUrl',
      value: function setGetTheAppUrl() {
        var clickUrls = undefined;
        if (this.canShowIpad()) {
          clickUrls = this.props.iPadClickUrls;
        } else {
          clickUrls = this.props.clickUrls;
        }
        var clickUrl = clickUrls[this.currentSport] || clickUrls[this.props.currentLeague] || clickUrls['default'];

        this.setState({ getTheAppUrl: clickUrl });
      }
    }, {
      key: 'isMobileDisplay',
      value: function isMobileDisplay() {
        if (this.canShowIpad()) {
          this.props.widthThreshold = 1025;
        }
        return $(window).width() < this.props.widthThreshold;
      }
    }, {
      key: 'isUsersFirstViewing',
      value: function isUsersFirstViewing() {
        return !$.cookie('interstitialShown');
      }
    }, {
      key: 'isUserFromTradeDeadline',
      value: function isUserFromTradeDeadline() {
        return !!document.referrer.match(/tradedeadline\.thescore\.com/);
      }
    }, {
      key: 'isUserFromDFS',
      value: function isUserFromDFS() {
        return _.contains(['/DFSnews', '/dfsnews'], this.props.referrer);
      }
    }, {
      key: 'isUserFromChatbot',
      value: function isUserFromChatbot() {
        return window.location.href.match(/chatbot=true/i);
      }
    }, {
      key: 'setStanzaAppUrl',
      value: function setStanzaAppUrl() {
        if (window.location.href.search(/stanza/) !== -1) {
          this.setState({ getTheAppUrl: 'http://m.onelink.me/c898101a' });
        }
      }
    }, {
      key: 'canShowInterstitial',
      value: function canShowInterstitial() {
        return this.isMobileDisplay() && this.isUsersFirstViewing() && !this.isUserFromChatbot() && !this.isUserFromTradeDeadline() && !this.isUserFromDFS();
      }
    }, {
      key: 'canShowIpad',
      value: function canShowIpad() {
        return _userAgent.isIpad && this.props.sports;
      }
    }, {
      key: 'clickGetTheApp',
      value: function clickGetTheApp(event) {
        if (this.canShowIpad()) {
          _eventTracking.send('marketing_funnel', 'get_app_interstitial_clicked', { 'nonInteraction': 1 });
        } else {
          _eventTracking.send('marketing_funnel', 'get_app_ipad_interstitial_clicked', { 'nonInteraction': 1 });
        }
        this.outbrainConversion();
        // window.open breaks on Android because it receives a 302 to app market uri
        // Workaround is to set the current tabs location to the 302-ed market://
        // and the existing tab handles the market app intent.
        window.location = this.state.getTheAppUrl;

        // Prevent bubbling to closeModal()
        event.stopPropagation();
      }
    }, {
      key: 'closeModal',
      value: function closeModal() {
