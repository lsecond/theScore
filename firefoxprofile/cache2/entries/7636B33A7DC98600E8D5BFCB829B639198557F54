        rendered: false
      };

      this.scrollFn = _.throttle(this.onWindowScroll, 250).bind(this);
      this.updateCommentsTextFn = this.updateCommentsText.bind(this);
    }

    _inherits(FacebookComments, _React$Component);

    _createClass(FacebookComments, [{
      key: 'componentDidMount',
      value: function componentDidMount() {
        $(window).on('scroll', this.scrollFn);

        if ($(window).width() < 768) {
          this.setState({ isMobile: true });
        }
      }
    }, {
      key: 'componentWillUnmount',
      value: function componentWillUnmount() {
        $(window).off('scroll', this.scrollFn);
      }
    }, {
      key: 'onWindowScroll',
      value: function onWindowScroll() {
        if (!this.refs.container) {
          return;
        }

        var container = this.refs.container.getDOMNode(),
            $container = $(container);

        if ($container.is(':in-viewport')) {
          this.setState({ inViewport: true });
          this.parseWidget();
        } else {
          this.setState({ inViewport: false });
        }
      }
    }, {
      key: 'parseWidget',
      value: function parseWidget() {
        if (this.state.rendered) {
          return;
        }

        var container = this.refs.container.getDOMNode();

        FB.XFBML.parse(container, this.updateCommentsTextFn);

        this.setState({ rendered: true });
      }
    }, {
      key: 'updateCommentsText',
      value: function updateCommentsText() {
        if (!this.refs.commentsCount) {
          return;
        }

        var commentsCountStoreNode = this.refs.commentsCountStore.getDOMNode(),
            commentsCountNode = this.refs.commentsCount.getDOMNode(),
            count = parseInt(commentsCountStoreNode.textContent);

        if (count === 0) {
          commentsCountNode.textContent = 'Start the Conversation!';
        } else {
          commentsCountNode.textContent = count + ' comments';
        }
      }
    }, {
      key: 'renderWidget',
      value: function renderWidget() {
        if (this.state.isMobile) {
          return this.renderMobile();
        } else {
          return this.renderDesktop();
        }
      }
    }, {
