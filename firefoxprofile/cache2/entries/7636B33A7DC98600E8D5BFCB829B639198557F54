        frontEndClientPagesEulaIosPath: Utils.route([], ["format"], [2, [2, [2, [2, [2, [2, [2, [2, [7, "/", false], [6, "pages", false]], [7, "/", false]], [6, "mobile", false]], [7, "/", false]], [6, "eula", false]], [7, "/", false]], [6, "ios", false]], [1, [2, [8, ".", false], [3, "format", false]], false]], arguments),
        // front_end_client_pages_open_source => /pages/mobile/open-source-software(.:format)
