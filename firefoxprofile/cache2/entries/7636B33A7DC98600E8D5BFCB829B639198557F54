define('front_end/initializers/youtube-analytics-init', ['exports', 'module', 'components/util/youtube', 'components/util/youtube-analytics'], function (exports, module, _componentsUtilYoutube, _componentsUtilYoutubeAnalytics) {
  'use strict';

  function _interopRequire(obj) { return obj && obj.__esModule ? obj['default'] : obj; }

  var _YoutubeAnalytics = _interopRequire(_componentsUtilYoutubeAnalytics);

  module.exports = function () {
    var config = {
      'events': {
        'Play': true,
        'Pause': true,
        'Watch to End': true
      },
      'percentageTracking': {
        'every': 25,
        'each': [10, 90]
      }
    },
        eventsFired = {
      'Play': true,
      'Pause': true,
      'Watch to End': true
    },
        forceSyntax = config.forceSyntax || 0,
        dataLayerName = config.dataLayerName || 'dataLayer';

    var key = undefined;
    for (key in config.events) {
      if (config.events.hasOwnProperty(key)) {
        eventsFired[key] = config.events[key];
      }
    }

    _componentsUtilYoutube.YoutubeEmbed.registerCallback(init);

    function init() {
      new _YoutubeAnalytics(config, eventsFired);
    }
  };
});(function() {

  Object.keys(require.entries).filter(function(name) {
    return !!name.match(/^front_end\/initializers\//);
  }).forEach(function(requirePath) {
    require(requirePath).default();
  });

})();
