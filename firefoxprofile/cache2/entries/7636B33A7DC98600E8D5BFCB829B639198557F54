  var SlideInBase = (function (_React$Component) {
    function SlideInBase() {
      _classCallCheck(this, SlideInBase);

      _get(Object.getPrototypeOf(SlideInBase.prototype), 'constructor', this).call(this);

      this.slides = ['article'];
      this.openSlideThreshold = 2400;
      this.openSlidePagePercentage = 0.6;
      this.closeSlideThreshold = 200;
      this.widthThreshold = 1024;

      // Initial State
      this.state = {
        open: false,
        slider: null,
        manuallyClosed: false
      };

      this.debouncedOnWindowScroll = _.debounce(this.onWindowScroll, 250).bind(this);
    }

    _inherits(SlideInBase, _React$Component);

    _createClass(SlideInBase, [{
      key: 'componentWillMount',
      value: function componentWillMount() {
        var slideIndex = Math.floor(Math.random() * this.slides.length),
            openLinkFunc = this.openLink.bind(this),
            sliderType = this.slides[slideIndex],
            contentDifference = $(document).height() - $(window).height();

        var sliderTag = undefined;

        if (contentDifference < this.openSlideThreshold) {
          this.openSlideThreshold = contentDifference * this.openSlidePagePercentage;
        }

        switch (sliderType) {
          case 'twitter':
            sliderTag = React.createElement(TwitterSlide, {
              openLink: openLinkFunc,
              linkUrl: this.props.twitterUrl,
              text: this.props.twitterText });
            break;
          case 'facebook':
            sliderTag = React.createElement(FacebookSlide, {
              openLink: openLinkFunc,
              linkUrl: this.props.facebookUrl,
              text: this.props.facebookText });
            break;
          case 'article':
            sliderTag = React.createElement(ArticleSlide, {
              openLink: openLinkFunc,
              article: this.props.article });
            break;
          default:
            sliderTag = React.createElement(NewsletterSlide, {
              openLink: openLinkFunc,
              loadingGif: this.props.loadingGif,
              messagingUrl: this.props.messagingUrl });
        }

        this.setState({
          slider: {
            type: sliderType,
            tag: sliderTag
          }
        });
      }
    }, {
      key: 'componentDidMount',
      value: function componentDidMount() {
        $(window).on('scroll', this.debouncedOnWindowScroll);
      }
    }, {
      key: 'componentWillUnmount',
      value: function componentWillUnmount() {
        $(window).off('scroll', this.debouncedOnWindowScroll);
      }
    }, {
      key: 'onWindowScroll',
      value: function onWindowScroll() {
        if (this.state.manuallyClosed) {
          return;
        }

        var scrollPos = $(window).scrollTop();

        if (scrollPos > this.openSlideThreshold && !this.state.open || scrollPos < this.closeSlideThreshold && this.state.open) {
          this.toggle();
        }
      }
    }, {
      key: 'toggle',
      value: function toggle() {
        var eventName = !this.state.open ? 'slider_opened' : 'slider_closed';

        this.setState({ open: !this.state.open });
        _eventTracking.send('marketing_funnel', eventName, { 'nonInteraction': 1 });
      }
    }, {
      key: 'userToggledSlider',
      value: function userToggledSlider() {
        this.setState({ manuallyClosed: this.state.open });
        this.toggle();
      }
    }, {
      key: 'openLink',
      value: function openLink(url) {
        var eventName = 'open_' + this.state.slider.type + '_url';

        _eventTracking.send('marketing_funnel', eventName, { 'nonInteraction': 1 });
        window.open(url, '_self');
      }
    }, {
      key: 'render',
      value: function render() {
        if ($(window).width() <= this.widthThreshold || !this.state.slider) {
          return React.createElement('div', null);
        }

        var isOpen = this.state.open,
            sliderType = this.state.slider.type,
            sliderTag = this.state.slider.tag,
            userToggledSliderFunc = this.userToggledSlider.bind(this);

        return React.createElement(
          'div',
          { className: 'slide-in slide-in--' + sliderType + ' ' + (isOpen ? 'slide-in--open' : 'slide-in--closed') },
          React.createElement(
            'div',
            { className: 'slide-in__container' },
            React.createElement(
              'button',
              { className: 'slide-in__toggle',
                onClick: userToggledSliderFunc },
              React.createElement('i', { className: 'icon icon-nav-' + (isOpen ? 'right' : 'left') })
            ),
            React.createElement(
              'button',
              { className: 'slide-in__close',
                onClick: userToggledSliderFunc },
              React.createElement('i', { className: 'icon icon-close' })
            ),
            React.createElement(
              'div',
              { className: 'slide-in__content' },
              sliderTag
            )
          )
        );
      }
    }]);

    return SlideInBase;
  })(React.Component);

