});define('components/util/api-get', ['exports', 'module'], function (exports, module) {
  'use strict';

  var apiOptions = {
    method: 'GET',
    dataType: 'json',
    headers: {
      'X-Api-Version': '1.7.10',
      'X-Country-Code': 'CA',
      'X-User-Agent': 'theScore (Web)'
    }
  };

  function merge(src, dest) {
    var value = undefined;

    Object.keys(src).forEach(function (key) {
      value = src[key];
      if (typeof value === 'object' && !Array.isArray(value)) {
        dest[key] = {};
        merge(value, dest[key]);
      } else {
        dest[key] = value;
      }
    });
  }

  module.exports = function (path) {
    var options = arguments[1] === undefined ? {} : arguments[1];

    /* eslint-disable */
    var apiUrl = 'http://api.thescore.com',

    /* eslint-enable */
    url = apiUrl + path,
        mergedOpts = {};

    merge(apiOptions, mergedOpts);
    merge(options, mergedOpts);

    return $.ajax(url, mergedOpts);
  };
