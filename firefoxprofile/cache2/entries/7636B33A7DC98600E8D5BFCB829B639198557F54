        // front_end_client_pages_open_source_android => /pages/mobile/open-source-software/android(.:format)
        // function(options)
        frontEndClientPagesOpenSourceAndroidPath: Utils.route([], ["format"], [2, [2, [2, [2, [2, [2, [2, [2, [7, "/", false], [6, "pages", false]], [7, "/", false]], [6, "mobile", false]], [7, "/", false]], [6, "open-source-software", false]], [7, "/", false]], [6, "android", false]], [1, [2, [8, ".", false], [3, "format", false]], false]], arguments),
        // front_end_client_pages_open_source_android_esports => /pages/mobile/open-source-software/android-esports(.:format)
        // function(options)
