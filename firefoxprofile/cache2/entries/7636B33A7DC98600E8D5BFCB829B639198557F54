});define('front_end/initializers/fb-sdk', ['exports', 'module'], function (exports, module) {
  'use strict';

  var fbRoot = null,
      fbEventsBound = false;

  function detachFbRoot() {
    fbRoot = $('#fb-root').detach();

    return fbRoot;
  }

  function restoreFacebookRoot() {
    if ($('#fb-root').length > 0) {
      return $('#fb-root').replaceWith(fbRoot);
    } else {
      return $('body').append(fbRoot);
    }
  }

  function parsePage() {
    if (typeof FB === 'undefined' || _.isNull(FB)) {
      _.delay(function () {
        FB.XFBML.parse();
      }, 1000);
      return;
    }

    FB.XFBML.parse();
  }

  function bindFacebookEvents() {
    $(document).on('page:fetch', detachFbRoot).on('page:change', restoreFacebookRoot).on('page:load', parsePage);

    fbEventsBound = true;

    return fbEventsBound;
  }

  module.exports = function () {
    if (!fbEventsBound) {
      bindFacebookEvents();
    }
  };
});define('front_end/initializers/fb_comment_tracking', ['exports', 'module', 'components/util/event-tracking'], function (exports, module, _componentsUtilEventTracking) {
  'use strict';

  function _interopRequire(obj) { return obj && obj.__esModule ? obj['default'] : obj; }

  var _eventTracking = _interopRequire(_componentsUtilEventTracking);

  function commentTracker(response) {
    _eventTracking.send('marketing_funnel', 'fb_comment_created', { 'nonInteraction': 1, 'url': response.href });
  }

  function parseComment() {
    if (typeof FB !== 'undefined') {
      FB.Event.subscribe('comment.create', commentTracker);
    } else {
      _.delay(function () {
        parseComment();
      }, 250);
    }
  }

  module.exports = function () {
    $(document).ready(function () {
      parseComment();
    });
  };
});define('front_end/initializers/fitvids', ['exports', 'module'], function (exports, module) {
  'use strict';

  module.exports = function () {
    $(document).ready(function () {
      $('.article-segment--embed').fitVids();
    });
  };
});define('front_end/initializers/instagram-embed', ['exports', 'module'], function (exports, module) {
  'use strict';

  var instagramEventsBound = undefined;

  function parseInstagramEmbeds() {
    if ($('.instagram-media').length === 0) {
      return;
    }

    if (typeof instgrm === 'undefined' || _.isNull(instgrm)) {
      _.delay(function () {
        parseInstagramEmbeds();
      }, 1000);
      return;
    }

    instgrm.Embeds.process();
  }

  function bindInstagramEvents() {
    $(document).on('page:load', parseInstagramEmbeds);

    instagramEventsBound = true;
  }

  module.exports = function () {
    if (!instagramEventsBound) {
      bindInstagramEvents();
    }
  };
});define('front_end/initializers/signup', ['exports', 'module'], function (exports, module) {
  'use strict';

  function validEmail(email) {
    var simpleEmailCheck = /[^@\s]+@[^@\s]+\.\w+/g;

    return email !== null && email.match(simpleEmailCheck);
  }

  module.exports = function () {
    $(document).ready(function () {
      $('#step-one button').click(function () {
        $('#step-one').hide();
        $('#step-two').show();
      });

      $('#step-two button').click(function () {
        var email = $('#email').val(),
            variation = $('.subscribe-form').data('variation');

        if (validEmail(email)) {
          /* eslint-disable */
          var messagingUrl = 'http://messaging.thescore.com';
          /* eslint-enable */

          $.ajax('' + messagingUrl + '/quickdraft_teaser_newsletter/subscribe', {
            type: 'post',
            dataType: 'json',
            data: {
              email: email,
              variation: variation
            }
          }).done(function () {
            $('#step-two').hide();
            $('#step-three').show();
          }).fail(function (jqXHR) {
            if (jqXHR && jqXHR.hasOwnProperty('responseJSON')) {
              $('#error').text(jqXHR.responseJSON.error);
            } else {
              $('#error').text('Something went wrong. Please try again.');
            }
          });
        } else {
          $('#error').text('Invalid email entered. Please try again.');
        }
      });
    });
  };
});define('front_end/initializers/track-page-view', ['exports', 'module', 'components/util/send-to-splunk'], function (exports, module, _componentsUtilSendToSplunk) {
  'use strict';

  function _interopRequire(obj) { return obj && obj.__esModule ? obj['default'] : obj; }

  var _sendToSplunk = _interopRequire(_componentsUtilSendToSplunk);

  module.exports = function () {
    $(document).on('page:change', function () {
      ga('send', 'pageview', window.location.pathname);
      _sendToSplunk('pageview');
    });
  };
});define('front_end/initializers/track-related-article', ['exports', 'module', 'components/util/event-tracking'], function (exports, module, _componentsUtilEventTracking) {
  'use strict';

  function _interopRequire(obj) { return obj && obj.__esModule ? obj['default'] : obj; }

