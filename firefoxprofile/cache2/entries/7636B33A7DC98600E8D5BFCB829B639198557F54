            searchResults = this.state.results,
            filters = [{ regex: TEAM_SUFFIX, text: TEAM_SUFFIX, className: TEAM_SUFFIX }, { regex: PLAYER_SUFFIX, text: PLAYER_SUFFIX, className: PLAYER_SUFFIX }, { regex: ARTICLE_SUFFIX, text: ARTICLE_SUFFIX, className: ARTICLE_SUFFIX }];

