          'data-href': props.url,
          'data-width': props.widgetSettings.width,
          'data-numposts': props.widgetSettings.numposts,
          'data-colorscheme': props.widgetSettings.colorscheme });
      }
    }, {
      key: 'render',
      value: function render() {
        var state = this.state;

        return React.createElement(
          'div',
          { className: 'article-facebook-comments', ref: 'container' },
          (state.inViewport || state.rendered) && this.renderWidget()
        );
      }
    }]);

    return FacebookComments;
  })(React.Component);

