      platform = _userAgent2.platform,

  /* eslint-disable */
  ktApi = new KontagentApi('c923fc0c27874826b9ff0908fbacbb66');
  /* eslint-enable */

  function callKontagent(category, action, url) {
    var kontagentMetadata = {
      'subtype1': action,
      'data': JSON.stringify({
        'location': url,
        'platform': platform
      })
    };
    try {
      ktApi.trackEvent(uuid, category, kontagentMetadata);
    } catch (e) {
      _reportError2('KontagentError', 'ktApi.trackEvent failed for:\n      ' + category + '\n      ' + JSON.stringify(kontagentMetadata));
    }
  }

  module.exports = callKontagent;
