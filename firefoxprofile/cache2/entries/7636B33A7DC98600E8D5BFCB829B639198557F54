    title: 'Top News'
  }, {
    url: _routes.frontEndDailyFantasyNewsPath(),
    title: 'Fantasy News'
  }, {
    url: _routes.frontEndAuthorsPath(),
    title: 'Writers'
  }];

  module.exports = links;
