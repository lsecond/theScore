  var _metaLeagues = _interopRequire(_utilMetaLeagues);

  var _routes = _interopRequire(_componentsUtilRoutes);

  var _logoTemplate = _interopRequire(_componentsUtilLogoTemplate);

  function linkHelper(protocol, hostname, port, league) {
    return protocol + '//' + hostname + (protocol === 'http:' && port === 80 || protocol === 'https:' && port === 443 ? '' : ':' + port) + '/' + league;
  }

  var ContextMenuItem = (function (_React$Component) {
    function ContextMenuItem() {
      _classCallCheck(this, ContextMenuItem);

      _get(Object.getPrototypeOf(ContextMenuItem.prototype), 'constructor', this).call(this);

      this.proxyClick = this.proxyClick.bind(this);
    }

    _inherits(ContextMenuItem, _React$Component);

    _createClass(ContextMenuItem, [{
      key: 'proxyClick',
      value: function proxyClick() {
        this.props.clickMenuItem(this.props.url);
      }
    }, {
      key: 'isNotNews',
      value: function isNotNews(url) {
        return !_.includes(url, 'news');
      }
    }, {
      key: 'render',
      value: function render() {
        return React.createElement(
          'a',
          { 'data-no-turbolink': this.isNotNews(this.props.url),
            href: this.props.url,
            onClick: this.proxyClick,
            className: 'context-menu-item' },
          React.createElement(
            'span',
            { className: 'context-menu-item-content' },
            this.props.children
          )
        );
      }
    }]);

    return ContextMenuItem;
  })(React.Component);

  var ContextMenu = (function (_React$Component2) {
    function ContextMenu() {
      _classCallCheck(this, ContextMenu);

      _get(Object.getPrototypeOf(ContextMenu.prototype), 'constructor', this).call(this);

      this.state = {
        open: false
      };

      this.toggle = this.toggle.bind(this);
    }

    _inherits(ContextMenu, _React$Component2);

    _createClass(ContextMenu, [{
      key: 'clickMenuItem',
      value: function clickMenuItem(url) {
        if (url.includes('news')) {
          Turbolinks.visit(_routes.frontEndLeagueNewsPath(this.props.league));
        } else {
          window.location.href(url);
        }
        event.stopPropagation();
      }
    }, {
      key: 'toggle',
      value: function toggle() {
        this.setState({ open: !this.state.open });
        $(document).trigger('closeMainMenu');
      }
    }, {
      key: 'render',
      value: function render() {
        var _this2 = this;

        var protocol = window.location.protocol,
            port = window.location.port,
            hostname = window.location.hostname,
            link = linkHelper(protocol, hostname, port, this.props.league),
            isNewsSection = this.props.activeSection === 'submissions' || this.props.activeSection === 'articles',
            hasLeague = this.props.league,
            hasSections = this.props.sections.length !== 0,
            hasOneSection = this.props.sections.length === 1,
            buttonClasses = ['context-menu-button', 'visible-xs', hasSections ? 'context-menu-toggle' : '', hasOneSection ? 'only-item' : ''].join(' ');

        return React.createElement(
          'nav',
          { className: 'context-menu ' + (this.state.open ? ' open' : '') },
          React.createElement(
            'button',
            { className: buttonClasses,
              onClick: this.toggle },
            React.createElement(
              'div',
              { className: 'header-title-wrapper' },
              hasLeague && React.createElement(
                'span',
                { className: 'title ff-regular fw-medium uppercase hidden-sm' },
                this.props.title
              ),
              !hasLeague && React.createElement('div', { dangerouslySetInnerHTML: { __html: _logoTemplate('condensed') } })
            ),
            hasSections && React.createElement(
              'span',
              { className: 'context-menu__section ff-regular fs-micro fw-semi-bold uppercase' },
              'News'
            )
          ),
          this.props.sections.map(function (section) {
            return React.createElement(
              ContextMenuItem,
              { url: link + '/' + section.key,
                clickMenuItem: _this2.clickMenuItem,
                className: 'context-menu-item',
                key: section.key },
              React.createElement(
                'span',
                { className: 'context-menu-link' + (isNewsSection && section.key === 'news' ? ' active' : '') },
                React.createElement('i', { className: 'icon-display icon-' + section.key }),
                React.createElement(
                  'span',
                  null,
                  section.name
                )
              )
            );
          })
        );
      }
    }]);

    return ContextMenu;
  })(React.Component);

  ContextMenu.defaultProps = {
    sections: []
  };

  var SubNavigation = (function (_React$Component3) {
    function SubNavigation() {
      _classCallCheck(this, SubNavigation);

      _get(Object.getPrototypeOf(SubNavigation.prototype), 'constructor', this).call(this);

      this.state = {
        open: false,
        sections: [],
        hasSections: false
      };

      this.onWindowScroll = this.onWindowScroll.bind(this);
    }

    _inherits(SubNavigation, _React$Component3);

    _createClass(SubNavigation, [{
      key: 'componentWillMount',
      value: function componentWillMount() {
        var _this3 = this;

        this.debouncedOnWindowScroll = _.debounce(this.onWindowScroll, 250);

        _metaLeagues.promise.then(function (result) {
          var allLeagues = result.all,
              leagueSlug = _this3.props.league,
              currentLeague = _.find(allLeagues, { slug: leagueSlug });

          if (!currentLeague) {
            return;
          }

          _this3.setState({ sections: currentLeague.sections });

          if (currentLeague.sections.length >= 1) {
            $('.app-content').addClass('has-sections');
            _this3.setState({ hasSections: true });
            _this3.show();
          }
        });
      }
    }, {
      key: 'componentDidMount',
      value: function componentDidMount() {
        $(window).on('scroll', this.debouncedOnWindowScroll);

        this.lastScrollPos = $(window).scrollTop();
      }
    }, {
      key: 'componentWillUnmount',
      value: function componentWillUnmount() {
        $(window).off('scroll', this.debouncedOnWindowScroll);
      }
    }, {
      key: 'findSectionsByLeagueSlug',
      value: function findSectionsByLeagueSlug(leaguesArray, leagueSlug) {
        var league = _.find(leaguesArray, { 'slug': leagueSlug });

        return league.sections;
      }
    }, {
      key: 'onWindowScroll',
      value: function onWindowScroll() {
        var scrollPos = $(window).scrollTop();

        if (!this.state.hasSections) {
          return;
        }

        if (scrollPos - this.lastScrollPos > 0) {
          this.hide();
        } else if (scrollPos - this.lastScrollPos < 0) {
          this.show();
        }

        this.lastScrollPos = scrollPos;
      }
    }, {
      key: 'hide',
      value: function hide() {
        this.setState({ open: false });

        // Have to manually add class to outside div due to react-rails
        $('.' + this.props.wrapperDivClassName).addClass('collapse');
      }
    }, {
      key: 'show',
      value: function show() {
        this.setState({ open: true });

        // Have to manually remove class to outside div due to react-rails
        $('.' + this.props.wrapperDivClassName).removeClass('collapse');
      }
    }, {
      key: 'render',
      value: function render() {
        return React.createElement(
          'div',
          { className: 'context-menu-row ' + (this.state.open ? '' : 'collapse') },
          React.createElement(
            'div',
            { className: 'container' },
            React.createElement(ContextMenu, { url: this.props.url,
              league: this.props.league,
              title: this.props.title,
              activeSection: this.props.activeSection,
              sections: this.state.sections })
          )
        );
      }
    }]);

    return SubNavigation;
  })(React.Component);

  module.exports = SubNavigation;
});define('components/util/default-home-section-links', ['exports', 'module', 'components/util/routes'], function (exports, module, _componentsUtilRoutes) {
  'use strict';

  function _interopRequire(obj) { return obj && obj.__esModule ? obj['default'] : obj; }

  var _routes = _interopRequire(_componentsUtilRoutes);

  var links = [{
    url: _routes.frontEndTrendingPath(),
    title: 'Trending'
  }, {
