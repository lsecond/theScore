          return false;
        }

        twttr.widgets.load(document.getElementById(this.props.containerId));

        return true;
      }
    }, {
      key: 'parseInstagram',
      value: function parseInstagram() {
        if (typeof instgrm === 'undefined' || _.isNull(instgrm)) {
          return false;
        }

        instgrm.Embeds.process();
        return true;
      }
    }, {
      key: 'onWindowScroll',
      value: function onWindowScroll() {
        var $window = $(window),
            windowScrollTop = $window.scrollTop(),
            $container = $('#' + this.props.containerId),
            containerBottom = $container.offset().top + $container.outerHeight(true);

        if (windowScrollTop >= containerBottom - $window.height()) {
          if (this.state.loadMore) {
            this.loadMore();
          }
        }
      }
    }, {
      key: 'render',
      value: function render() {
        var entries = this.state.entries.map(function (entry) {
          return React.createElement(LiveBlogEntry, { key: entry.id, entry: entry });
